Checks: '-*,readability-function-size'
WarningsAsErrors: ''
HeaderFileExtensions:         ['h']
ImplementationFileExtensions: ['c']
CheckOptions:
  - key:             readability-function-size.LineThreshold
    value:           '30'
  - key:             readability-function-size.UseLinesOfCode
    value:           'true'
  - key:             readability-function-size.IncludeCommentsInLinesOfCode
    value:           'false'
  - key:             readability-function-size.IncludeEmptyLinesInLinesOfCode
    value:           'false'
